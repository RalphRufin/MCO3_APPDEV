<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Student Reservation</title>
</head>
<body>
    <h1>Student Reservation</h1>
    <ul>
        <% reservedSlotReservations.forEach(reservation => { %>
            <li>
                Lab <%= reservation.lab %>,
                Seat <%= reservation.seat %>,
                <%= reservation.date %>,
                <%= reservation.timeSlot %> Time Slot,
                Reserved by: <%= reservation.reservee %>
            </li>
        <% }); %>
    </ul>
    <select id="navigateSelect">
        <option value="/labs">Labs</option>
        <option value="/studentLabs">Student Labs</option>
    </select>

    <script>
        document.getElementById('navigateSelect').addEventListener('change', () => {
            const selectedOption = document.getElementById('navigateSelect').value;
            window.location.href = selectedOption;
        });
    </script>
</body>
</html>
